<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../partials/head'); %>
    </head>
    <body>
        <div class="wrapper">
            <header id="up">
                <%- include('../partials/header'); %>
            </header>
            <main>
                <% if (admin) { %>
                    <div class="manager">
                        <div class="create_form">

                        </div>
                        <div class="list">
                            <div class="list_obj" id="add" activate="off" form="track">
                                <p class="large_ft">Add tack+</p>
                            </div>
                            <form id="search_track">
                                <input type="text" name="name" list="tracks" placeholder="Music search by albums" autocomplete="off">
                                <datalist id="tracks">
                                    <% albums.forEach(element => { %>
                                     <option value="<%= element.title %>"></option>
                                    <% }) %>
                                </datalist>
                                <button>Search</button>
                            </form>
                           <div class="result">

                           </div>
                        </div>
                    </div>
                <% } %>
            </main>
            <footer>
                <%- include('../partials/footer'); %>
            </footer>
        </div>
    </body>
</html>
