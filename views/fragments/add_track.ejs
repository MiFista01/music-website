<form id="add_row" method="post" enctype="multipart/form-data">
    <h2 class="large_ft">Add tarck</h2>
    <input type="text" name="title" placeholder="Name" autocomplete="off" required >
    <input type="text" name="album" list="albums" placeholder="Album name" autocomplete="off" required>
    <datalist id="albums">
        <% albums.forEach(element => { %>
            <option value="<%= element.title%>"></option>
        <% }) %>
    </datalist>
    <input type="file" name="music" id="music" required>
    <p class="middle_ft">Warning: filename must be unique</p>
    <button>Add track</button>
    <p class="middle_ft error_text"></p>
</form>